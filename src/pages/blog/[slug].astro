---
import { posts } from '../../lib/ghost';

export async function getStaticPaths() {
  return posts.map((post) => {
    return {
      params: {
        slug: post.slug,
      },
      props: post
    };
  });
}

const { title, html } = Astro.props;
---

<h1>{title}</h1>

<Fragment set:html={html} />